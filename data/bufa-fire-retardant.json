  "glossar": [
    {
      "begriff": "ATH",
      "vollstaendigerName": "Aluminiumhydroxid",
      "definition": "Eine Verbindung, die bei Temperaturen über 200°C in Wasser zerfällt, den Brandherd durch Verdampfung kühlt und gleichzeitig den Sauerstoffzutritt zur Flamme verhindert."
    },
    {
      "begriff": "Intumeszenz",
      "definition": "Bildung einer aufgeschäumten, sauerstoffundurchlässigen Schicht durch Hitzeeinwirkung auf einen Kunststoff. Der Schutzschaum verhindert das Eindringen von Sauerstoff und schirmt das darunter liegende Laminat für eine gewisse Zeit von der Hitze ab."
    },
    {
      "begriff": "Karbonisierung",
      "definition": "Gezieltes „Verkohlen\" einer Oberfläche durch eine entsprechend wirksame Brandschutzausrüstung. Die dünne Kohlenstoffschicht verhindert den Kontakt der darunter liegenden Laminatbestandteile mit Sauerstoff."
    },
    {
      "begriff": "EN 45545-2",
      "definition": "Die Norm, die die Anforderungen an das Brandverhalten von Werkstoffen und Komponenten im Schienenverkehr regelt. Sie gilt als extrem streng."
    },
    {
      "begriff": "HL",
      "vollstaendigerName": "Hazard Level",
      "definition": "Ein Klassifizierungssystem, das hilft, die Anwendungsbereiche von Kunststoffbauteilen nach der von ihnen im Brandfall geforderten Mindestleistung zu ordnen. Die strengste Stufe ist HL3."
    },
    {
      "begriff": "LOI",
      "vollstaendigerName": "Limiting Oxygen Index",
      "definition": "Ein wichtiger Indikator zur Beschreibung des Brandverhaltens von Kunststoffen. Die Zahl gibt an, wie viel Sauerstoff der Werkstoff benötigt, um sich zu entzünden. Ab einem LOI von 21 gilt ein Werkstoff an der Luft als schwer entflammbar."
    }
  ],
